			/** Header Navigation and Top Menu ***/

.button.is-link:hover {
    color: #fffc!important;
    box-shadow: none;
}

.nav-right {
    text-transform: uppercase;
}

.header-main .nav > li > a {
    line-height: 50px;
    font-weight: 500;
}

.nav-dropdown {
    text-align: left;
    display: table-cell;
    white-space: nowrap;
    width: 360px;
    min-width: 360px;
}

.nav>li>a, .nav-dropdown>li>a, .nav-column>li>a {
    color: rgb(0 0 0);
    transition: all .2s;
    font-weight: 500;
}

/*** Sermon Page List Study Guides ***/

body.single-sermon h1 a {
	color: #fff;
}

ul.sermon-guide-list {
    list-style: none;
}

ul.sermon-guide-list li {
    float: left;
    margin-right: 10px;
}

.youtube-div iframe {
	width: 100%;
	height: 330px;
	margin-bottom: 30px;
}

.has-hover:hover .tag-label {
    opacity: 1;
    background-color: #383870;
}


/*** Sermons Archive Layout ***/

#post-list > div:nth-child(1) {
    max-width: 100%;
    float: none;
}

.all-sermons-audio .box-none .box-text {
    display: block!important;
}

audio:focus {
    outline: none;
}

audio {
	margin: 20px 0px;
}

/** Homepage more sermons **/

.more-sermon-box .box-text {
  height: 180px;
}

/** Sermon Page **/

.sermon-date {
	margin-left: 5px;
}

